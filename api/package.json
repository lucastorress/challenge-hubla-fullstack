{
  "name": "hubla-api",
  "version": "1.0.0",
  "description": "Rest API repository using NodeJS, Express.js, TypeScript, Swagger and SOLID",
  "main": "src/app.ts",
  "engines": {
    "node": ">= 16.13 <17"
  },
  "scripts": {
    "start:prod": "node build/app.js",
    "start:dev": "ts-node-dev --transpile-only -r tsconfig-paths/register --ignore-watch node_modules src/app.ts",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.build.json",
    "test": "jest",
    "prettier": "prettier --config .prettierrc --write .",
    "test:watch": "jest --watch",
    "swagger-autogen": "ts-node-dev --transpile-only src/docs/swagger.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucastorress/challenge-hubla-fullstack.git"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "solid",
    "tests-with-jest",
    "docker",
    "jwt-token",
    "api rest",
    "swagger"
  ],
  "author": "Lucas P. Torres de AraÃºjo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucastorress/challenge-hubla-fullstack/issues"
  },
  "homepage": "https://github.com/lucastorress/challenge-hubla-fullstack#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "swagger-ui-express": "^4.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@swc/core": "^1.2.207",
    "@swc/jest": "^0.2.21",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.10",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.5.3",
    "jest": "^28.1.1",
    "prettier": "^2.7.1",
    "supertest": "^6.2.3",
    "swagger-autogen": "^2.21.4",
    "ts-jest": "^28.0.5",
    "ts-node": "10.7.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "4.5.2"
  }
}
